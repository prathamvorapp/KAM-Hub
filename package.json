{
  "name": "kam-dashboard-unified",
  "version": "1.0.0",
  "description": "KAM Dashboard - Unified Next.js Application with Convex",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"NEXT,CADDY\" --prefix-colors \"green,magenta\" \"next dev -p 3022\" \"node scripts/start-caddy.js 8000\"",
    "dev:next": "next dev -p 3022",
    "build": "next build",
    "start": "next start -p 3022",
    "start:prod": "concurrently --names \"NEXT,CADDY\" --prefix-colors \"green,blue\" \"next start -p 3022\" \"node scripts/start-caddy.js 3000\"",
    "test": "node scripts/test-services.js",
    "lint": "next lint",
    "caddy:start": "node scripts/start-caddy.js 0",
    "caddy:validate": "caddy\\caddy.exe validate --config Caddyfile",
    "caddy:format": "caddy\\caddy.exe fmt --overwrite Caddyfile",
    "security:audit": "npm audit",
    "security:audit-fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/forms": "^0.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "autoprefixer": "^10.0.1",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "csrf": "^3.1.0",
    "csv-parser": "^3.2.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.24.12",
    "isomorphic-dompurify": "^3.0.0-rc.2",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.294.0",
    "multer": "^2.0.2",
    "next": "^16.1.6",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.15.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0"
  },
  "keywords": [
    "kam",
    "dashboard",
    "nextjs",
    "convex",
    "unified"
  ],
  "author": "KAM Dashboard Team",
  "license": "MIT"
}
